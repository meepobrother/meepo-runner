{
    "name": "meepo-runner",
    "version": "2.4.6",
    "description": "runner for angular",
    "main": "./bundles/meepo.umd.js",
    "module": "./esm5/meepo.js",
    "es2015": "./esm2015/meepo.js",
    "typings": "./meepo.d.ts",
    "scripts": {
        "build": "bash ./build.sh",
        "git": "git add . && git commit -m 'save to back' && git push origin master",
        "test": "node index.js",
        "demo": "ng serve --port 4018 --open",
        "server": "pm2 start index.js --name meepo",
        "reload": "pm2 restart all && pm2 logs meepo",
        "delete": "pm2 delete all",
        "logs": "pm2 logs meepo",
        "build2": "ng build --prod -d ../addons/imeepos_runner/template/mobile/v2/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/meepobrother/meepo-runner.git"
    },
    "keywords": [],
    "author": "imeepos",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/meepobrother/meepo-runner/issues"
    },
    "homepage": "https://github.com/meepobrother/meepo-runner/README.md",
    "devDependencies": {
        "@angular/cli": "^1.6.1",
        "@angular/common": "latest",
        "@angular/compiler": ">=5.1.2",
        "@angular/compiler-cli": ">=5.1.2",
        "@angular/core": "latest",
        "@angular/platform-browser": "latest",
        "@angular/platform-browser-dynamic": ">=5.1.2",
        "@types/node": "^8.5.1",
        "less": "^2.7.3",
        "node-sass": "^4.7.2",
        "pm2": "^2.9.1",
        "reflect-metadata": "^0.1.10",
        "rollup": "^0.52.1",
        "rollup-plugin-node-resolve": "^3.0.0",
        "rollup-plugin-replace": "^2.0.0",
        "rollup-plugin-sourcemaps": "^0.4.2",
        "rollup-plugin-uglify": "^2.0.1",
        "rxjs": "^5.5.5",
        "ts-node": "^4.0.2",
        "typescript": "~2.4.2",
        "wepayui": "^0.1.8",
        "zone.js": "0.8.4"
    },
    "dependencies": {
        "angular-web-storage": "*",
        "meepo-axios": "*",
        "meepo-base64": "*",
        "meepo-bmap": "*",
        "meepo-core": "*",
        "meepo-event": "*",
        "meepo-forms": "*",
        "meepo-jssdk": "*",
        "meepo-picker": "^1.4.6",
        "meepo-ua": "*",
        "meepo-uuid": "*",
        "meepo-xscroll": "*",
        "mobile-select": "*"
    }
}
